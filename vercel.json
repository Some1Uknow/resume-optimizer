{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "functions": {
      "api/generate-pdf": {
        "memory": 3008,
        "maxDuration": 30,
        "includeFiles": "node_modules/@sparticuz/chromium/**"
      }
    },
    "routes": [
      {
        "src": "/api/generate-pdf",
        "methods": ["POST"],
        "dest": "/api/generate-pdf"
      }
    ]
  }