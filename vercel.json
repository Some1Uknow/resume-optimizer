{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": { "nodeVersion": "18.x" }
      }
    ],
    "functions": {
      "api/generate-pdf": {
        "memory": 3008,
        "maxDuration": 30,
        "handler": "api/generate-pdf/route.ts",
        "includeFiles": "node_modules/@sparticuz/chromium/**"
      }
    },
    "routes": [
      {
        "src": "/api/generate-pdf",
        "methods": ["POST"],
        "dest": "/api/generate-pdf"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }