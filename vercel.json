{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "functions": {
      "api/generate-pdf": {
        "memory": 3008,
        "maxDuration": 30,
        "includeFiles": [
          "components/resume/**",
          "public/tailwind.css"
        ]
      }
    },
    "routes": [
      {
        "src": "/api/generate-pdf",
        "methods": ["POST"],
        "dest": "/api/generate-pdf"
      }
    ]
  }